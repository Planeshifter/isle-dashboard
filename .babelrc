{
    "presets": [
        [ "@babel/preset-env", {
            "targets": {
                "node": "current"
            }
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        "add-module-exports",
        "transform-react-jsx"
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs",
                "css-modules-transform"
            ]
        }
    }
}
