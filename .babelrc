{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "plugins": [
        "add-module-exports",
        "transform-react-jsx"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "webpack-loaders", {
                        "config": "config/webpack.config.test.js",
                        "verbose": false
                    }
                ],
                [
                    "resolver", {
                        "resolveDirs": [
                            "src",
                            "node_modules/@stdlib/stdlib/lib/node_modules"
                        ]
                    }
                ],
                "css-modules-transform"
            ]
        }
    }
}